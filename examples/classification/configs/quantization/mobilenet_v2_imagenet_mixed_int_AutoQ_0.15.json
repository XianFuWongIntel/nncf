{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
      "sample_size": [2, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 128,
    // "train_subset_ratio": 0.02, // should we move as local setting of the AutoQ initializer?
    "val_subset_ratio": 0.2,    // should we move as local setting of the AutoQ initializer?
    "epochs": 5,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.00001,
        "schedule_type": "multistep",
        "steps": [
            5
        ]
    },
    "target_device": "NONE",
    "compression": {
        "algorithm": "quantization",
        "weights": {
            "mode": "asymmetric",
            "bits": 8,
            "per_channel": true
        },
        "activations": {
            "mode": "asymmetric",
            "bits": 8,
            "per_channel": false
        },
        "initializer": {
            "batchnorm_adaptation": {
                "num_bn_adaptation_steps": 50
            },
            "range":
            {
                "type": "mean_min_max",
                "num_init_steps": 50
            },
            "precision": {
                "type": "autoq",
                "bits": [2, 4, 8],
                "iter_number": 600,
                "compression_ratio": 0.15,
                "num_steps_per_iter": 80, 
            }
        }
    }
}
